  "require": {
    "php": "^8.3",
    "slim/slim": "^4.12",
    "slim/psr7": "^1.6",
    "php-di/php-di": "^7.0",
    "php-di/slim-bridge": "^3.4",
    "ecotone/ecotone": "^1.213",
    "cuyz/valinor": "^1.9",
